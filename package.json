  "keywords": [
    "csound",
    "csound-wasm",
    "wasm",
    "webassembly",
    "audio",
    "webaudio"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "nix-shell nix/xshell.nix",
    "lint": "eslint 'src/**/*.js'",
    "watch": "webpack --watch --mode=develop",
